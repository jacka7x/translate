{
    "manifest_version": 3,
    "version": "0.0",
    "name": "PLACEHOLDER_EXT_NAME",
    
    "icons": {
        // X size
        // XX size
    },

    "action": {
        // "browser_style": true,
        // "default_icon": {
        //   "16": "button/geo-16.png",
        //   "32": "button/geo-32.png"
        // },
        "default_title": "DEFAULT_TITLE",
        "default_popup": "/popup/popup.html"
    },

    "permissions": [
        "activeTab",
        "scripting",
        "storage"
    ],

    "background": {
        "service_worker": "background.js"
    },

    "content_scripts": [
        {   
            "matches": ["<all_urls>"],
            "js": ["content-script.js"]
        }
    ]
}